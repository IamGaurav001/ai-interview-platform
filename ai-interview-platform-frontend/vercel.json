{
  "redirects": [
    {
      "source": "/:path*",
      "has": [
        {
          "type": "host",
          "value": "prephire.co.in"
        }
      ],
      "destination": "https://www.prephire.co/:path*",
      "permanent": true
    },
    {
      "source": "/:path*",
      "has": [
        {
          "type": "host",
          "value": "www.prephire.co.in"
        }
      ],
      "destination": "https://www.prephire.co/:path*",
      "permanent": true
    }
  ],
  "rewrites": [
    {
      "source": "/api/:match*",
      "destination": "https://api.prephire.co/api/:match*"
    },
    {
      "source": "/audio/:match*",
      "destination": "https://api.prephire.co/audio/:match*"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}
